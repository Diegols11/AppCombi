@model IEnumerable<AppCombi.Models.Ruta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreRuta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Llegada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Minuto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreRuta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Llegada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Minuto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RutaID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RutaID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RutaID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
